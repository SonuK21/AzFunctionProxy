{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "proxyHome": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/"
      },
      "backendUri": "https://yoursampletodoappuri/index.html"
    },
    "proxyContent": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/content/{*all}"
      },
      "backendUri": "https://yoursampletodoappuri/content/{all}"
    }
  }
}
