{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "proxyHome": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/"
      },
      "backendUri": "https://azfa001storage.z16.web.core.windows.net/index.html"
    },
    "proxyContent": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/content/{*all}"
      },
      "backendUri": "https://azfa001storage.z16.web.core.windows.net/content/{all}"
    }
  }
}
